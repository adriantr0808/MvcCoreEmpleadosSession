@model List<int>

    <h1>Resumen de la compra</h1>

@{
    
    List<Empleado> emps = ViewData["EMPLEADOS"] as List<Empleado>;


}
    <ul>
        @for (int i = 0; i < emps.Count; i++)
        {
                <li>@emps[i].No_Emp - @emps[i].Apellido - @(Model[i] * @emps[i].Salario)</li>
            }
        
    </ul>